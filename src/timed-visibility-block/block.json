{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "timed-vblck/timed-visibility-block",
	"version": "1.0.0",
	"title": "Timed Visibility Block",
	"category": "widgets",
	"icon": "clock",
	"description": "Control when your content shines. Perfect for announcements, promotions, and time-sensitive information!",
	"attributes": {
		"visibleFrom": {
			"type": "string",
			"default": ""
		},
		"visibleUntil": {
			"type": "string",
			"default": ""
		},
		"visibilityType": {
			"type": "string",
			"default": "show"
		},
		"timeOnly": {
			"type": "boolean",
			"default": false
		},
		"fallbackMessage": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "timed-visibility-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
